from itertools import permutations

#Final Function Pass#
def sum_func(x, n):
    perm = list(permutations(x,2))
    sum_tup = [x+y for (x,y) in perm]
    truth_table = [True if x == n else False for x in sum_tup]

    for i in range(len(truth_table)):
        if(truth_table[i]):
            print "True at pair: " , perm[i]
